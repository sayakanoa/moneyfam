<h1>カレンダー画面</h1>

<%= month_calendar events: @events do |date, events| %>
<%= date.day %>


<% events.each do |event| %>
    <div>
      <%= event.title %>
      <%= event.body %></br>
      <%= link_to '削除', event_path(event.id), method: :delete %>
    </div>
<% end %>

<% end %>

<%= link_to 'イベント追加', new_event_path %>